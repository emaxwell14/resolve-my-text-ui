{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 120
            }
        ]
    }
}